<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teacher</title>
  </head>
  <body>
    <h1>Teachers Data</h1>
    <table>
      <thead>
        <tr>
          <th>Id</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(teacher => { %>
          <tr>
            <td><%= teacher.id %></td>
            <td><%= teacher.first_name %></td>
            <td><%= teacher.last_name %></td>
            <td><%= teacher.email %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
