<%- include layout/head %>
  <div class="container">
    <div class="text-center">
      <h1>Give Score to: <%= student.full_name %></h1>
    </div>
    <form action="/subjects/<%= subjectId %>/givescore/<%= student.id %>" method="post">
      <div class="form-group">
        <label >Score</label>
        <input type="number" class="form-control" placeholder="Score" name="score">
      </div>
      <div class="form-group">
        <input class="btn btn-primary" type="submit" value="Give Score">
      </div>
    </form>
  </div>
<%- include layout/foot %>
